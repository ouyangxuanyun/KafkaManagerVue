    "/getclusters": {
      "get": {
        "summary": "获取集群列表",
        "description": "所有集群的信息，包括集群名称，kafka版本，zookeeper地址",
        "tags": [
          "Cluster"
        ],
        "responses": {
          "200": {
            "description": "响应成功",
            "schema": {
              "type": "object",
              "properties": {
                "clusters": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "description": [
                      "name",
                      "kafkaVersion",
                      "zkHosts",
                      "operation"
                    ]
                  }
                },
                "Title": {
                  "type": "string",
                  "description": "显示页面标签"
                }
              }
            }
          }
        }
      }
    },
    "/getCreateClusterFailed": {
